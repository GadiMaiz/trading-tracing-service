{
    "presets": [
        [
            "env",
            {
                "targets": {
                    "node": "current"
                }
            }
        ]
    ],
    "plugins": [
        [
            "module-resolver",
            {
                "root": [
                    "./src"
                ],
                "alias": {
                    "utils": "./src/utils",
                    "server": "./src/server",
                    "db": "./src/dal",
                    "logger": "./src/utils/logger",
                    "handlerDeligator" : "./src/server/modules/handlerDeligator" ,
                    "Bitstamp" : "./src/server/thirdParty/bitstamp/Bitstamp",
                    "TickerStream" : "./src/server/thirdParty/bitstamp/streams/TickerStream",
                    "status" : "./src/utils/status"
                }
            }
        ]
    ]
}